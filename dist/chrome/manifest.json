{
  "manifest_version": 2,
  "name": "Favioli",
  "description": "Emoji favicons for the web",
  "version": "1.1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  }
}