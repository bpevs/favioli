{
  "manifest_version": 2,
  "name": "Favioli",
  "description": "Emoji Favicons for the web",
  "version": "0.3.0",
  "background": {
    "scripts": [ "build/background.js" ]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "build/contentScript.js" ],
      "run_at": "document_end"
    }
  ],
  "options_page": "build/options.html",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  }
}
