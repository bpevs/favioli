{
  "manifest_version": 2,

  "name": "Favioli",
  "description": "Emoji Favicons for the web",
  "version": "0.0.1",

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "build/contentScript.js" ],
      "run_at": "document_end"
    }
  ],

  "options_page": "build/options.html",

  "permissions": [
    "activeTab",
    "storage"
  ]
}